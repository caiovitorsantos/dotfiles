[user]
	name = Caio Vitor Santos
	email = caio.santos@guideinvestimentos.com.br
[alias]
  hist = log --graph --pretty=\"format:%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset\" --all
  st = status
  s = status -s
  amend = commit --amend
  a = add -A
  co = checkout
  cm = commit -m
  dc = diff --cached
  di = diff
  b = branch
  ap = add --patch
  sm = submodule
  rb = rebase
  l = log --pretty=\"format:%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset\" --all
[push]
	default = matching
